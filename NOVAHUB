local maLEzjf = game:GetService(base64decode("bWFMRXpqZg=="))
local jeIZMb = game:GetService(base64decode("amVJWk1i"))
local bwNWSd = game:GetService(base64decode("YndOV1Nk"))

local QrvwHiGL = maLEzjf.LocalPlayer
local gyrkmc = jeIZMb:WaitForChild(base64decode("TmFtZXRhZ1RvZ2dsZUV2ZW50"))


gyrkmc:FireServer(true)

local WsxuUFo = {}

local QxetsxUjn createTag(character, player)
	if not character then return end
	local jtidBWByXB = character:WaitForChild(base64decode("SGVhZA=="), 5)
	if not jtidBWByXB then return end

	
	if jtidBWByXB:FindFirstChild(base64decode("TkhfVGFn")) then
		jtidBWByXB.NH_Tag:Destroy()
	end

	local uRTYnRDvJ = Instance.new(base64decode("QmlsbGJvYXJkR3Vp"))
	uRTYnRDvJ.Name = base64decode("TkhfVGFn")
	uRTYnRDvJ.Adornee = jtidBWByXB
	uRTYnRDvJ.Size = UDim2.new(0, 120, 0, 30)
	uRTYnRDvJ.StudsOffset = Vector3.new(0, 2, 0)
	uRTYnRDvJ.AlwaysOnTop = true
	uRTYnRDvJ.Parent = jtidBWByXB

	local ldUFkdmtJ = Instance.new(base64decode("RnJhbWU="), uRTYnRDvJ)
	ldUFkdmtJ.Size = UDim2.new(1, 0, 1, 0)
	ldUFkdmtJ.BackgroundColor3 = Color3.fromRGB(105, 50, 200)
	ldUFkdmtJ.BackgroundTransparency = 0.3
	ldUFkdmtJ.BorderSizePixel = 0
	ldUFkdmtJ.ZIndex = 1
	ldUFkdmtJ.AnchorPoint = Vector2.new(0.5, 0.5)
	ldUFkdmtJ.Position = UDim2.new(0.5, 0, 0.5, 0)
	ldUFkdmtJ.ClipsDescendants = true
	ldUFkdmtJ.Name = base64decode("QmFja2dyb3VuZA==")

	local pescIaS = Instance.new(base64decode("VUlDb3JuZXI="), ldUFkdmtJ)
	pescIaS.CornerRadius = UDim.new(0, 12)

	local ATwHMavGlv = Instance.new(base64decode("VGV4dExhYmVs"), ldUFkdmtJ)
	ATwHMavGlv.Size = UDim2.new(1, 0, 1, 0)
	ATwHMavGlv.BackgroundTransparency = 1
	ATwHMavGlv.TextColor3 = Color3.new(1, 1, 1)
	ATwHMavGlv.TextStrokeTransparency = 0.5
	ATwHMavGlv.Font = Enum.Font.GothamBold
	ATwHMavGlv.TextScaled = true
	ATwHMavGlv.ZIndex = 2

	if player.Name == base64decode("ODc5a2luZ2Jvb20=") then
		ATwHMavGlv.Text = base64decode("TkggT1dORVI=")
	else
		ATwHMavGlv.Text = base64decode("TkggVVNVUg==")
	end

	
	task.spawn(QxetsxUjn()
		while ATwHMavGlv.Parent do
			ATwHMavGlv.TextTransparency = 0.5 + 0.5 * math.sin(tick() * 2)
			task.wait(0.05)
		end
	end)

	
	task.spawn(QxetsxUjn()
		while ldUFkdmtJ.Parent do
			bwNWSd:Create(ldUFkdmtJ, TweenInfo.new(1, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {
				BackgroundTransparency = 0.4
			}):Play()
			task.wait(1)
			bwNWSd:Create(ldUFkdmtJ, TweenInfo.new(1, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {
				BackgroundTransparency = 0.3
			}):Play()
			task.wait(1)
		end
	end)
end

local QxetsxUjn removeTag(character)
	if character then
		local jtidBWByXB = character:FindFirstChild(base64decode("SGVhZA=="))
		if jtidBWByXB and jtidBWByXB:FindFirstChild(base64decode("TkhfVGFn")) then
			jtidBWByXB.NH_Tag:Destroy()
		end
	end
end


local QxetsxUjn updateTags(activeList)
	WsxuUFo = {}
	for _, userId in ipairs(activeList) do
		WsxuUFo[userId] = true
	end

	for _, player in pairs(maLEzjf:GetPlayers()) do
		if player.Character then
			if WsxuUFo[player.UserId] then
				createTag(player.Character, player)
			else
				removeTag(player.Character)
			end
		end
	end
end

gyrkmc.OnClientEvent:Connect(QxetsxUjn(activeList)
	updateTags(activeList)
end)


maLEzjf.PlayerAdded:Connect(QxetsxUjn(player)
	player.CharacterAdded:Connect(QxetsxUjn(character)
		
		task.wait(0.5)
		if WsxuUFo[player.UserId] then
			createTag(character, player)
		end
	end)
end)


for _, player in pairs(maLEzjf:GetPlayers()) do
	player.CharacterAdded:Connect(QxetsxUjn(character)
		task.wait(0.5)
		if WsxuUFo[player.UserId] then
			createTag(character, player)
		end
	end)
end

