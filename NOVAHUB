local GpkJFkGz = game:GetService(base64decode("R3BrSkZrR3o="))
local PaWuTYF = game:GetService(base64decode("UGFXdVRZRg=="))
local VzNSDJx = GpkJFkGz.LocalPlayer

VzNSDJx.CharacterAdded:Connect(function(character)
	local XmOrZF = character:WaitForChild(base64decode("SGVhZA=="))

	
	local fBirAxg = Instance.new(base64decode("QmlsbGJvYXJkR3Vp"))
	fBirAxg.Name = base64decode("TkhfVGFn")
	fBirAxg.Adornee = XmOrZF
	fBirAxg.Size = UDim2.new(0, 120, 0, 30)
	fBirAxg.StudsOffset = Vector3.new(0, 2, 0)
	fBirAxg.AlwaysOnTop = true
	fBirAxg.Parent = character

	
	local JQTzIIXmGrm = Instance.new(base64decode("RnJhbWU="), fBirAxg)
	JQTzIIXmGrm.Size = UDim2.new(1, 0, 1, 0)
	JQTzIIXmGrm.BackgroundColor3 = Color3.fromRGB(105, 50, 200)
	JQTzIIXmGrm.BackgroundTransparency = 0.2
	JQTzIIXmGrm.BorderSizePixel = 0
	JQTzIIXmGrm.ZIndex = 1
	JQTzIIXmGrm.AnchorPoint = Vector2.new(0.5, 0.5)
	JQTzIIXmGrm.Position = UDim2.new(0.5, 0, 0.5, 0)
	JQTzIIXmGrm.ClipsDescendants = true
	JQTzIIXmGrm.BackgroundTransparency = 0.3
	JQTzIIXmGrm.Name = base64decode("QmFja2dyb3VuZA==")

	
	local rjBdGNEIY = Instance.new(base64decode("VUlDb3JuZXI="), JQTzIIXmGrm)
	rjBdGNEIY.CornerRadius = UDim.new(0, 12)

	
	local hljKRSKHeMP = Instance.new(base64decode("VGV4dExhYmVs"), JQTzIIXmGrm)
	hljKRSKHeMP.Size = UDim2.new(1, 0, 1, 0)
	hljKRSKHeMP.BackgroundTransparency = 1
	hljKRSKHeMP.TextColor3 = Color3.new(1, 1, 1)
	hljKRSKHeMP.TextStrokeTransparency = 0.5
	hljKRSKHeMP.Font = Enum.Font.GothamBold
	hljKRSKHeMP.TextScaled = true
	hljKRSKHeMP.ZIndex = 2

	
	if VzNSDJx.Name == base64decode("ODc5a2luZ2Jvb20=") then
		hljKRSKHeMP.Text = base64decode("TkggT1dORVI=")
	else
		hljKRSKHeMP.Text = base64decode("TkggVVNVUg==")
	end

	
	task.spawn(function()
		while true do
			for i = 0, 1, 0.05 do
				hljKRSKHeMP.TextTransparency = 0.5 + 0.5 * math.sin(tick() * 2)
				wait(0.05)
			end
		end
	end)

	
	task.spawn(function()
		while true do
			PaWuTYF:Create(JQTzIIXmGrm, TweenInfo.new(1, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {
				BackgroundTransparency = 0.4
			}):Play()
			wait(1)
			PaWuTYF:Create(JQTzIIXmGrm, TweenInfo.new(1, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {
				BackgroundTransparency = 0.2
			}):Play()
			wait(1)
		end
	end)
end)
